{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "npm run clean && npm run build ng-cron && node prepare-package.mjs",
    "build-lib-alpha": "ALPHA=1 npm run build-lib",
    "clean": "rm -rf dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npm run build-lib && npm run e2e-inject && npm run build e2e && cypress open --component",
    "e2e-run": "npm run build-lib && npm run e2e-inject && npm run build e2e && cypress run --component",
    "e2e-inject": "npm install ./dist/ng-cron --force",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "publish-alpha": "npm publish ./dist/ng-cron --tag alpha --access=public",
    "publish": "npm publish ./dist/ng-cron --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@sbzen/cron-core": "2.0.2",
    "bootstrap": "^5.2.0",
    "bootstrap-legacy": "npm:bootstrap@^4.6.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular/cli": "~15.1.6",
    "@angular/compiler-cli": "^15.1.0",
    "@babel/core": "^7.20.12",
    "@compodoc/compodoc": "^1.1.19",
    "@sbzen/e2e": "https://github.com/ua-cron/e2e/releases/download/20230106035216/dist.tar.gz",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/angular": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "~18.13.0",
    "@types/jasmine": "~4.3.0",
    "babel-loader": "^8.2.5",
    "cypress": "^12.5.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.1.2",
    "typescript": "~4.9.4"
  }
}
